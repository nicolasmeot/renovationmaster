<div class="display">
    {{>nav}}

    <h1 class="sectionHeadLarge">Hello, this is the page with all of your projects</h1>

    <div class="projectsContainer">
        <ul> 
            {{#each projectsFromDB}}
                <li><a href="/projects/{{id}}">{{>projectcard}}</a></li>
            {{/each}}
        </ul>

    </div>
    

    <a href="/projects/new">Create new project</a>

    <img id="addProject" src="../images/addGradient.svg" alt="addProject">


</div>

